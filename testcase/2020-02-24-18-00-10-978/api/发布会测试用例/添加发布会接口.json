{
    "name": "添加发布会接口",
    "noReset": true,
    "output": [],
    "request": {
        "data": {
            "address": "${get_random_address()}",
            "eid": "${get_random_int()}",
            "limit": "$limit",
            "name": "${get_random_name()}发布会",
